        <b class="tooltip tooltip-bottom-right">請輸入稅率</b></label>
      </label>
    </section>
    <section class="col col-lg-3 col-md-3 col-sm-3 col-xs-3">
      <label class="label">稅金</label>
      <label class="input state-disabled">
        <input id="tax" v-model="data.item.tax" value="{{ calcTax() }}" readonly>
      </label>
    </section>
  </div>

