  <div class="row">
    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12 notForCreate">
      <label class="label">供應商列表</label>
      <label class="textarea state-disabled">
        <textarea rows="2" v-model="data.item.suppliersNameArray" disabled>
        </textarea>
      </label>
    </section>

    <section class="col col-lg-12 col-md-12 col-sm-12 col-xs-12 notForCreate">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>產品分類</label>
      <label class="textarea state-disabled">
        <textarea rows="2" v-model="data.item.categoriesNameArray" disabled>
        </textarea>
      </label>
    </section>

    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>公開狀態</label>
      <label class="input state-disabled switch"></label>
      <span class="input-group-addon">
        <span class="onoffswitch">
          <input type="checkbox" name="publish" class="onoffswitch-checkbox" v-model="data.item.publish" disabled="disabled" id="publish">
          <label class="onoffswitch-label" for="publish">
            <span
              class="onoffswitch-inner"
              data-swchon-text="On"
              data-swchoff-text="Off"></span>
            <span class="onoffswitch-switch"></span>
          </label>
        </span>
      </span>
        <b class="tooltip tooltip-bottom-right">publish</b></label>
    </section>

    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4 fn-show-hidden">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>產品供應商</label>
      <label class="select state-disabled">
        <select style="height: 60px;" class="form-control" multiple="multiple" name="suppliersId" title="選擇產品供應商" v-model="data.item.suppliersId">
          <option v-for=" supplier in data.option.supplierList" v-bind:value="supplier.id" @click="addSupplier">
            {{supplier.name}}
          </option>
        </select>
      </label>
    </section>

    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4 fn-show-hidden">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>產品分類</label>
      <label class="select state-disabled">
        <select style="height: 60px; "class="form-control" multiple="multiple" name="categoriesId" title="選擇產品分類" v-model="data.item.categoriesId">
          <option v-for=" category in data.option.categoryList" v-bind:value="category.id" @click="addCategory">
            {{category.CategoryDescription.name}}
          </option>
        </select>
      </label>
    </section>
  </div>


  <div class="row notForCreate">
    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <label class="label">建立時間</label>
