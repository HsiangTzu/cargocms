      <label class="label"><span class="text-danger fn-show-hidden">*</span>單位</label>
      <label class="input state-disabled">
        <input type="text" name="unit" placeholder="" v-model="data.item.unit" disabled>
        <b class="tooltip tooltip-bottom-right">請輸入單位</b></label>
      </label>
    </section>

    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>最小存量</label>
      <label class="input state-disabled">
        <input type="number" name="minimum" min="0" placeholder="" v-model="data.item.minimum" disabled>
        <b class="tooltip tooltip-bottom-right">請輸入最小存量</b></label>
      </label>
    </section>

    <section class="col col-lg-4 col-md-4 col-sm-4 col-xs-4">
      <label class="label"><span class="text-danger fn-show-hidden">*</span>產地</label>
      <label class="input state-disabled">
        <input type="text" name="origin" placeholder="" v-model="data.item.origin" disabled>
        <b class="tooltip tooltip-bottom-right">請輸入產地</b></label>
      </label>
    </section>

